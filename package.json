{
  "name": "tsp-photo",
  "version": "1.0.0",
  "description": "Photography Website",
  "main": "index.js",
  "scripts": {
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",

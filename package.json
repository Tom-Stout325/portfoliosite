    "onchange": "^7.1.0",
    "parallelshell": "3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  }
}
